@using MvcHeritageCustomMasonry.Controllers
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Heritage Custom Masonry</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @Html.Partial("Partial_Navbar_Fixed_Top")
    <article class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p><small>&copy; @DateTime.Now.Year - Heritage Custom Masonry</small></p>
        </footer>
    </article>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
